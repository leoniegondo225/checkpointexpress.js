                            <div class="mb-3">
                                <label for="exampleFormControlInput1" class="form-label fw-bold">Email address</label>
                                <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="Entrez votre adress">
                              </div>
                          </div>
                          <div  class="col-lg-6">
                            <div class="mb-3">
                                <label for="exampleFormControlInput1" class="form-label fw-bold">votre contact</label>
                                <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="Entrez votre contact">
                              </div>
                          </div>
                    
                </div>
             </div>
           </div>
           
        
           <div>
            <button type="button" class="btn btn-primary m-2">Envoyer</button>
           </div>
        </section>
      


      <script src="./vendors/bootstrap/js/bootstrap.bundle.js"></script>
